{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/event.js",
    "app/(tabs)/Home.js",
    "app/(tabs)/Group.js",
    "app/(tabs)/_layout.js"
  ]
}
